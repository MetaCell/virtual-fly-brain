{
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "env": {
        "production":{
            "presets": ["minify"]
        }
    },
    "plugins": [
        "@babel/transform-regenerator",
        "@babel/plugin-transform-runtime",
        "@babel/plugin-proposal-class-properties",
        [
            "module-resolver", {
                "root": ["./src"]
            }
        ]
    ]
}
